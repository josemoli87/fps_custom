<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_vehicle_form_view" model="ir.ui.view">
            <field name="name">res.config.settings.vehicle.form.view</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="40"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <app data-string="Vehicle Repair" string="Vehicle Repair" name="tk_advance_vehicle_repair">
                        <block title="Slot Booking Details">
                            <setting help="Slot Booking for Website">
                                <field name="website_slot_booking"/>
                            </setting>
                        </block>
                    </app>
                </xpath>
            </field>
        </record>
    </data>
</odoo>