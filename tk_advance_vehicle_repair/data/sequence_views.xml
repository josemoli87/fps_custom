<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="vehicle_booking_number_sequence" model="ir.sequence">
            <field name="name">Booking Number</field>
            <field name="code">vehicle.booking</field>
            <field name="prefix">BN/VE-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="vehicle_inspection_number_sequence" model="ir.sequence">
            <field name="name">Inspection Number</field>
            <field name="code">inspection.job.card</field>
            <field name="prefix">INSP/JC-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="vehicle_repair_number_sequence" model="ir.sequence">
            <field name="name">Repair Number</field>
            <field name="code">repair.job.card</field>
            <field name="prefix">VEH/JC-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>